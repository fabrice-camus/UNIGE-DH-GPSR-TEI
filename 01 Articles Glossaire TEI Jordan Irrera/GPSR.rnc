  att.datcat.attributes, att.lexicographic.attribute.opt
att.lexicographic.attribute.opt =
  
  ## (optional) indicates whether the element is optional or not
  [ a:defaultValue = "false" ] attribute opt { xsd:boolean }?
model.entryLike = entry
model.formPart = form | orth | pron | model.gramPart
model.gramPart = model.lexicalRefinement | model.morphLike
model.lexicalRefinement = gramGrp | usg | lbl
model.lexicalRefinement_alternation = gramGrp | usg | lbl
model.lexicalRefinement_sequence = gramGrp, usg, lbl
model.lexicalRefinement_sequenceOptional = gramGrp?, usg?, lbl?
model.lexicalRefinement_sequenceOptionalRepeatable =
  gramGrp*, usg*, lbl*
model.lexicalRefinement_sequenceRepeatable = gramGrp+, usg+, lbl+
model.morphLike = notAllowed
model.morphLike_alternation = notAllowed
model.morphLike_sequence = empty
model.morphLike_sequenceOptional = empty
model.morphLike_sequenceOptionalRepeatable = empty
model.morphLike_sequenceRepeatable = notAllowed
model.ptrLike.form = notAllowed
att.global.analytic.attributes = att.global.analytic.attribute.ana
att.global.analytic.attribute.ana =
  [
    a:documentation [
      "(analysis) indicates one or more elements containing interpretations of the element on which the "
      ns1:code [ "@ana" ]
      " attribute appears."
    ]
  ]
  attribute ana {
    list { xsd:anyURI+ }
  }?
start = TEI
